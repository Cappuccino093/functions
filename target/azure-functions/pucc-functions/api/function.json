{
  "scriptFile" : "../pucc-functions-1.0.jar",
  "entryPoint" : "com.sigel.pucc.functions.functions.api.Api.run",
  "bindings" : [ {
    "type" : "httpTrigger",
    "direction" : "in",
    "name" : "req",
    "route" : "api/{*route}",
    "methods" : [ "GET", "HEAD", "POST", "PUT", "DELETE", "CONNECT", "OPTIONS", "TRACE", "PATCH" ],
    "authLevel" : "FUNCTION"
  }, {
    "type" : "http",
    "direction" : "out",
    "name" : "$return"
  } ]
}